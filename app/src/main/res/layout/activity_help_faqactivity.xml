<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activitys.HelpFAQActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:elevation="@dimen/dp_4"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="28dp"
            android:layout_marginBottom="@dimen/dp_5"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/activity_helpfaq_back_img"
                android:layout_width="24dp"
                android:layout_height="match_parent"
                android:layout_marginRight="30dp"
                app:srcCompat="?attr/homeAsUpIndicator"
                app:tint="@color/material_500" />

            <TextView
                android:layout_width="255dp"
                android:layout_height="match_parent"
                android:text="@string/help_label"
                android:textColor="@color/material_500"
                android:textSize="@dimen/dp_20"
                android:textStyle="bold" />

        </LinearLayout>
    </androidx.appcompat.widget.Toolbar>

    <LinearLayout
        android:id="@+id/activity_helpfaq_title1_linearlayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginVertical="30dp"
        android:background="@drawable/custom_border"
        android:paddingHorizontal="10dp"
        android:paddingVertical="10dp">

        <TextView
            android:id="@+id/activity_helpfaq_title1_text"
            android:layout_width="312dp"
            android:layout_height="wrap_content"
            android:layout_weight="10"
            android:text="How do I access the compiler ?"
            android:textColor="@color/material_500"
            android:textSize="20dp"
            android:textStyle="bold" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/activity_helpfaq_title2_linearlayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/custom_border"
        android:paddingHorizontal="10dp"
        android:paddingVertical="10dp">

        <TextView
            android:id="@+id/activity_helpfaq_title2_text"
            android:layout_width="312dp"
            android:layout_height="wrap_content"
            android:layout_weight="10"
            android:text="How to access Programs?"
            android:textColor="@color/material_500"
            android:textSize="20dp"
            android:textStyle="bold" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/activity_helpfaq_title3_linearlayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/custom_border"
        android:paddingHorizontal="10dp"
        android:paddingVertical="10dp">

        <TextView
            android:id="@+id/activity_helpfaq_title3_text"
            android:layout_width="312dp"
            android:layout_height="wrap_content"
            android:layout_weight="10"
            android:text="How to access Programs?"
            android:textColor="@color/material_500"
            android:textSize="20dp"
            android:textStyle="bold" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/activity_helpfaq_title5_linearlayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/custom_border"
        android:paddingHorizontal="10dp"
        android:paddingVertical="10dp">

        <TextView
            android:id="@+id/activity_helpfaq_title5_text"
            android:layout_width="312dp"
            android:layout_height="wrap_content"
            android:layout_weight="10"
            android:text="How to enroll into?"
            android:textColor="@color/material_500"
            android:textSize="20dp"
            android:textStyle="bold" />

    </LinearLayout>


</LinearLayout>